{
  "name": "gatsby-graphql-source-toolkit",
  "version": "3.0.0-alpha.2",
  "description": "A toolkit designed to simplify data sourcing from a remote GraphQL API into Gatsby.",
  "author": "Vladimir Razuvaev <vladimir@gatsbyjs.com>",
  "repository": {
    "url": "https://github.com/nrandell/gatsby-graphql-toolkit"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist/* && tsc",
    "prepublishOnly": "npm run build",
    "format": "prettier --list-different --write ./src"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "gatsby",
    "graphql"
  ],
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "fs-extra": "^10.1.0",
