    "node-fetch": "^3.2.10",
    "p-queue": "^7.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "gatsby": "^5.0.0",
    "graphql": "^16.6.0",
    "jest": "^29.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0 || ^5.0.0"
  }
}
